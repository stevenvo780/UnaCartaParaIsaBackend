{
  "id": null,
  "uid": "spatial-indexes-performance",
  "title": "8. Spatial Indexes - Performance Analysis",
  "tags": ["simulation", "performance", "spatial", "indexes", "queries"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "type": "row",
      "title": "Query Performance",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Spatial Query Duration (Avg & Max)",
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "backend_spatial_query_duration_ms{stat=\"avg\"}",
          "legendFormat": "{{index}} (avg)",
          "refId": "A"
        },
        {
          "expr": "backend_spatial_query_duration_ms{stat=\"max\"}",
          "legendFormat": "{{index}} (max)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byFrameRefID", "options": "B" },
            "properties": [
              {
                "id": "custom.lineStyle",
                "value": { "dash": [10, 10], "fill": "dash" }
              },
              {
                "id": "color",
                "value": { "mode": "fixed", "fixedColor": "red" }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["mean", "max", "lastNotNull"],
          "sortBy": "Mean",
          "sortDesc": true
        }
      }
    },
    {
      "type": "stat",
      "title": "Total Spatial Queries",
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(backend_spatial_query_count_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "type": "stat",
      "title": "Avg Query Rate (1m)",
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(backend_spatial_query_count_total[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 1000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "type": "bargauge",
      "title": "Avg Results per Query (Eficiencia)",
      "gridPos": { "h": 5, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "backend_spatial_avg_results_per_query",
          "legendFormat": "{{index}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 50, "color": "orange" },
              { "value": 100, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      }
    },
    {
      "type": "timeseries",
      "title": "Query Rate por Index (queries/sec)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 11 },
      "targets": [
        {
          "expr": "rate(backend_spatial_query_count_total[1m])",
          "legendFormat": "{{index}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "qps",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15
          }
        }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Avg Results per Query (Over Time)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 11 },
      "targets": [
        {
          "expr": "backend_spatial_avg_results_per_query",
          "legendFormat": "{{index}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "max", "lastNotNull"]
        }
      }
    },
    {
      "type": "row",
      "title": "Index Rebuilds & Maintenance",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 19 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Total Index Rebuilds",
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "sum(backend_spatial_rebuild_count_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 1000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Total Rebuild Time",
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 20 },
      "targets": [
        {
          "expr": "sum(backend_spatial_rebuild_duration_ms)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 10000, "color": "yellow" },
              { "value": 60000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Avg Rebuild Duration",
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 20 },
      "targets": [
        {
          "expr": "sum(backend_spatial_rebuild_duration_ms) / sum(backend_spatial_rebuild_count_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 50, "color": "yellow" },
              { "value": 200, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "auto"
      }
    },
    {
      "type": "timeseries",
      "title": "Index Rebuild Rate (rebuilds/min)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 25 },
      "targets": [
        {
          "expr": "rate(backend_spatial_rebuild_count_total[1m])",
          "legendFormat": "{{index}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15
          }
        }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "type": "bargauge",
      "title": "Total Rebuild Time per Index",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 25 },
      "targets": [
        {
          "expr": "backend_spatial_rebuild_duration_ms",
          "legendFormat": "{{index}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 5000, "color": "yellow" },
              { "value": 20000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      }
    },
    {
      "type": "row",
      "title": "Detailed Index Statistics",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 33 },
      "collapsed": false
    },
    {
      "type": "table",
      "title": "Spatial Index Summary",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 34 },
      "targets": [
        {
          "expr": "backend_spatial_query_count_total",
          "format": "table",
          "refId": "A",
          "instant": true
        },
        {
          "expr": "backend_spatial_query_duration_ms{stat=\"avg\"}",
          "format": "table",
          "refId": "B",
          "instant": true
        },
        {
          "expr": "backend_spatial_query_duration_ms{stat=\"max\"}",
          "format": "table",
          "refId": "C",
          "instant": true
        },
        {
          "expr": "backend_spatial_avg_results_per_query",
          "format": "table",
          "refId": "D",
          "instant": true
        },
        {
          "expr": "backend_spatial_rebuild_count_total",
          "format": "table",
          "refId": "E",
          "instant": true
        },
        {
          "expr": "backend_spatial_rebuild_duration_ms",
          "format": "table",
          "refId": "F",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "merge"
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "stat": true
            },
            "renameByName": {
              "index": "Index",
              "Value #A": "Total Queries",
              "Value #B": "Avg Query (ms)",
              "Value #C": "Max Query (ms)",
              "Value #D": "Avg Results",
              "Value #E": "Rebuilds",
              "Value #F": "Total Rebuild Time (ms)"
            }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Total Queries",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "center",
            "filterable": true
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Avg Query (ms)" },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "decimals",
                "value": 4
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background",
                  "mode": "gradient"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "value": null, "color": "green" },
                    { "value": 0.1, "color": "yellow" },
                    { "value": 1, "color": "red" }
                  ]
                }
              }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Avg Results" },
            "properties": [
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      }
    }
  ]
}

{
  "id": null,
  "uid": "runtime-overview",
  "title": "Simulation Runtime Overview",
  "schemaVersion": 38,
  "version": 1,
  "timezone": "browser",
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Tick Duration (avg ms)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "backend_tick_duration_ms",
          "legendFormat": "{{rate}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "System Hotspots (avg ms)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "topk(10, backend_system_execution_ms)",
          "legendFormat": "{{system}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Entities en scheduler",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "backend_scheduler_entity_count",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Memoria usada (MB)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 8
      },
      "targets": [
        {
          "expr": "backend_memory_bytes{type=\"heapUsed\"} / 1024 / 1024",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Event Loop Utilization",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "backend_event_loop_utilization",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Active Agents",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "backend_active_agents_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "thresholds"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Total Resources",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 4,
        "y": 12
      },
      "targets": [
        {
          "expr": "backend_total_resources",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "thresholds"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Total Buildings",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 8,
        "y": 12
      },
      "targets": [
        {
          "expr": "backend_total_buildings",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "thresholds"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "Deep Profiling",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Subsystem Duration (Avg ms)",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 17
      },
      "targets": [
        {
          "expr": "backend_subsystem_duration_ms",
          "legendFormat": "{{system}}:{{operation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Subsystem Calls (Total)",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 25
      },
      "targets": [
        {
          "expr": "increase(backend_subsystem_calls_total[1m])",
          "legendFormat": "{{system}}:{{operation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "piechart",
      "title": "Event Loop Usage Breakdown",
      "gridPos": {
        "h": 12,
        "w": 12,
        "x": 0,
        "y": 33
      },
      "targets": [
        {
          "expr": "sum by (system, operation) (increase(backend_subsystem_total_duration_ms[1m]))",
          "legendFormat": "{{system}}:{{operation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "options": {
        "pieType": "donut",
        "reduceOptions": {
          "calcs": [
            "mean"
          ],
          "fields": "",
          "values": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "list",
          "placement": "right",
          "showLegend": true
        }
      }
    },
    {
      "type": "bargauge",
      "title": "Event Loop Usage Ranking",
      "gridPos": {
        "h": 12,
        "w": 12,
        "x": 12,
        "y": 33
      },
      "targets": [
        {
          "expr": "topk(10, sum by (system, operation) (increase(backend_subsystem_total_duration_ms[1m])))",
          "legendFormat": "{{system}}:{{operation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": []
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "showUnfilled": true,
        "reduceOptions": {
          "calcs": [
            "mean"
          ],
          "fields": "",
          "values": true
        },
        "legend": {
          "displayMode": "list",
          "placement": "right",
          "showLegend": true
        }
      }
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  }
}
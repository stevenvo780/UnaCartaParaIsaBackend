{
  "id": null,
  "uid": "batch-processing-efficiency",
  "title": "7. Batch Processing & GPU Efficiency",
  "tags": ["simulation", "performance", "batch", "gpu", "optimization"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "type": "row",
      "title": "GPU vs CPU Execution",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "GPU Utilization Ratio",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "backend_gpu_utilization",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 0.3, "color": "yellow" },
              { "value": 0.7, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "GPU vs CPU Executions Rate",
      "gridPos": { "h": 6, "w": 16, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "rate(backend_batch_executions_total{type=\"gpu\"}[1m])",
          "legendFormat": "{{processor}} (GPU)",
          "refId": "A"
        },
        {
          "expr": "rate(backend_batch_executions_total{type=\"cpu\"}[1m])",
          "legendFormat": "{{processor}} (CPU)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byFrameRefID", "options": "A" },
            "properties": [
              {
                "id": "color",
                "value": { "mode": "fixed", "fixedColor": "green" }
              }
            ]
          },
          {
            "matcher": { "id": "byFrameRefID", "options": "B" },
            "properties": [
              {
                "id": "color",
                "value": { "mode": "fixed", "fixedColor": "orange" }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "type": "bargauge",
      "title": "GPU Utilization Ratio por Processor",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 7 },
      "targets": [
        {
          "expr": "backend_batch_gpu_utilization_ratio",
          "legendFormat": "{{processor}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 0.2, "color": "orange" },
              { "value": 0.5, "color": "yellow" },
              { "value": 0.8, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      }
    },
    {
      "type": "piechart",
      "title": "Distribuci√≥n GPU vs CPU (Total Executions)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 7 },
      "targets": [
        {
          "expr": "sum by (type) (backend_batch_executions_total)",
          "legendFormat": "{{type}}",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": ["value", "percent"]
        },
        "pieType": "donut",
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "type": "row",
      "title": "Batch Sizes y Throughput",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 15 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Average Batch Size por Processor",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "backend_batch_avg_size",
          "legendFormat": "{{processor}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "auto"
          }
        }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "max", "lastNotNull"]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Entidades Procesadas en Batches (rate 1m)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "rate(backend_batch_entities_total[1m])",
          "legendFormat": "{{processor}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 15
          }
        }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "type": "stat",
      "title": "Total Entities Batched (Animal)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "backend_batch_entities_total{processor=\"animal\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 10000, "color": "yellow" },
              { "value": 100000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Total Entities Batched (Movement)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 24 },
      "targets": [
        {
          "expr": "backend_batch_entities_total{processor=\"movement\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 10000, "color": "yellow" },
              { "value": 100000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Total Entities Batched (Needs)",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 24 },
      "targets": [
        {
          "expr": "backend_batch_entities_total{processor=\"needs\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 10000, "color": "yellow" },
              { "value": 100000, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Current Batch Sizes",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 24 },
      "targets": [
        {
          "expr": "backend_batch_size",
          "legendFormat": "{{processor}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "type": "row",
      "title": "Batch Processing Performance",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 28 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Avg Batch Processing Duration",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 29 },
      "targets": [
        {
          "expr": "backend_batch_avg_duration_ms",
          "legendFormat": "{{processor}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "max", "lastNotNull"]
        }
      }
    },
    {
      "type": "table",
      "title": "Batch Processing Summary",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 29 },
      "targets": [
        {
          "expr": "backend_batch_avg_size",
          "format": "table",
          "refId": "A",
          "instant": true
        },
        {
          "expr": "backend_batch_avg_duration_ms",
          "format": "table",
          "refId": "B",
          "instant": true
        },
        {
          "expr": "backend_batch_gpu_utilization_ratio",
          "format": "table",
          "refId": "C",
          "instant": true
        },
        {
          "expr": "backend_batch_entities_total",
          "format": "table",
          "refId": "D",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "merge"
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "processor": "Processor",
              "Value #A": "Avg Batch Size",
              "Value #B": "Avg Duration (ms)",
              "Value #C": "GPU Ratio",
              "Value #D": "Total Entities"
            }
          }
        }
      ],
      "options": {
        "showHeader": true
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "center"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "GPU Ratio" },
            "properties": [
              {
                "id": "unit",
                "value": "percentunit"
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background",
                  "mode": "gradient"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "value": null, "color": "red" },
                    { "value": 0.5, "color": "yellow" },
                    { "value": 0.8, "color": "green" }
                  ]
                }
              }
            ]
          }
        ]
      }
    }
  ]
}
